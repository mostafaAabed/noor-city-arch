{
    "name": "Mobile Backend API",
    "api_id": "keyless",
    "org_id": "default",
    "definition": {
        "location": "header",
        "key": "version"
    },
    "use_keyless": true,
    "version_data": {
        "not_versioned": true,
        "versions": {
            "Default": {
                "name": "Default"
            }
        }
    },
    "custom_middleware": {
        "driver": "otto",
        "pre": [
          {
            "name": "injectHeaderMW",
            "path": "./middleware/injectHeader.js",
            "require_session": false,
            "raw_body_only": false
          }
        ]
  },
    "proxy": {
        "listen_path": "/mobile/",
        "target_url": "http://172.17.0.1:3000/api",
        "strip_listen_path": true
    }
}
